{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "db": "start http://localhost/phpmyadmin/index.php",
        "start": "yarn open && php artisan serve",
        "open": "start http://127.0.0.1:8000/",
        "mgrt": "php artisan migrate",
        "seed": "php artisan db:seed",
        "livewire": "composer require livewire/livewire",
        "ui-install": "composer require laravel/ui",
        "ui": "php artisan ui:auth",
        "breeze": "composer require laravel/breeze --dev",
        "breeze-install": "php artisan breeze:install",
        "laratrust": "composer require santigarcor/laratrust",
        "laratrust-setup": "php artisan laratrust:setup",
        "autoload": "composer dump-autoload",
        "laratrust-seeder": "php artisan laratrust:seeder",
        "controller": "php artisan make:controller UsersController --resource",
        "model": "php artisan make:model <File name> -m",
        "middleware": "php artisan make:middleware <File name>Middleware",
        "migrate": "php artisan make:migration <File name>_table",
        "request": "php artisan make:request ColorFormRequest"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/typography": "^0.5.0",
        "autoprefixer": "^10.4.7",
        "axios": "^1.1.2",
        "laravel-vite-plugin": "^0.8.0",
        "postcss": "^8.4.14",
        "tailwindcss": "^3.1.0",
        "vite": "^4.0.0"
    }
}
